@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";
}


@Html.Raw(umbraco.library.RenderMacroContent(CurrentPage.slider, CurrentPage.Id))


@{

    var home = Model.Content.Site();
    foreach (var childPage in home.Children.Where("DocumentTypeAlias =\"LandingPage\" and Visible and Name != \"Search\""))
    {
        Html.RenderPartial("Features", childPage);
    }
    
    
}



@CurrentPage.GetGridHtml("content", "biztech")